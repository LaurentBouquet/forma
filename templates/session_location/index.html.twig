{% extends 'base.html.twig' %}

{% block title %}SessionLocation index{% endblock %}

{% block body %}
    <h1>SessionLocation index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Street</th>
                <th>Postal_code</th>
                <th>City</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for session_location in session_locations %}
            <tr>
                <td>{{ session_location.id }}</td>
                <td>{{ session_location.street }}</td>
                <td>{{ session_location.postalCode }}</td>
                <td>{{ session_location.city }}</td>
                <td>
                    <a href="{{ path('session_location_show', {'id': session_location.id}) }}">show</a>
                    <a href="{{ path('session_location_edit', {'id': session_location.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('session_location_new') }}">Create new</a>
{% endblock %}
